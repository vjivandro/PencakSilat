<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pencaksilat.app.pencaksilatapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SejarahActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/page_sejarah"/>
        <activity
            android:name=".activity.TeknikDasarActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/page_teknik"/>
        <activity
            android:name=".activity.KombinasiActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/page_kombinasi"/>
        <activity
            android:name=".activity.InfoAplikasi"
            android:parentActivityName=".MainActivity"
            android:label="@string/page_info"/>
    </application>

</manifest>